{
  "name": "pornhub.js",
  "version": "1.0.0",
  "packageManager": "pnpm@7.13.4",
  "description": "Powerful PornHub API for Node.js",
  "author": "Pionxzh <pionxzh@csie.io>",
  "license": "MIT",
  "homepage": "https://github.com/pionxzh/pornhub.js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pionxzh/pornhub.js.git"
  },
  "bugs": {
    "url": "https://github.com/pionxzh/pornhub.js/issues"
  },
  "keywords": [
    "node.js",
    "pornhub",
    "pornhub-api",
    "hubtraffic",
    "webmaster"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=7.0.0"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "pretest": "npm run lint",
    "test": "vitest run",
    "test:type": "tsc --noEmit",
    "test:watch": "vitest",
    "prepublishOnly": "rm -rf ./dist && pnpm run build && clean-publish",
    "postpublish": "rm -rf ./dist"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "debug": "^4.3.4",
    "node-fetch": "^3.2.10",
    "urlcat": "^2.0.4"
  },
  "devDependencies": {
    "@pionxzh/eslint-config-ts": "^0.1.1",
    "@types/cheerio": "^0.22.31",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.8.4",
    "clean-publish": "^4.0.1",
    "eslint": "^8.25.0",
    "esno": "^0.16.3",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.7",
    "vitest": "^0.24.3"
  },
  "publishConfig": {
    "directory": "tmp_release"
  },
  "clean-publish": {
    "withoutPublish": true,
    "tempDir": "tmp_release"
  }
}
